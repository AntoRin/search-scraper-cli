{
   "name": "search-scraper-cli",
   "version": "1.0.0",
   "description": "A recursive scraper for your terminal",
   "main": "dist/index.js",
   "bin": {
      "search": "./bin/cli",
      "lookup": "./bin/cli"
   },
   "preferGlobal": true,
   "scripts": {
      "build": "rm -rf ./dist && tsc",
      "dev": "rm -rf ./dist && tsc -w"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/AntoRin/search-scraper-cli.git"
   },
   "keywords": [
      "shell-search",
      "node.js",
      "typescript"
   ],
   "author": "AntoRin",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/AntoRin/search-scraper-cli/issues"
   },
   "homepage": "https://github.com/AntoRin/search-scraper-cli#readme",
   "dependencies": {
      "minimist": "^1.2.5",
      "node-fetch": "^2.6.1"
   },
   "devDependencies": {
      "@types/minimist": "^1.2.1",
      "@types/node": "^15.12.4",
      "@types/node-fetch": "^2.5.10",
      "typescript": "^4.3.4"
   }
}
